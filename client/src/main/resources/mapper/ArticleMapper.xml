<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dan.dandelion.client.dao.ArticleDao">


  <select id="queryArticleByUniID" resultType="com.dan.dandelion.client.model.Article" parameterType="java.lang.Integer">
    SELECT *
    FROM mini_article m
    WHERE m.uni_id = #{uniID}
  </select>

  <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
    select
    id,name,status,user
    from book
    where id = #{id,jdbcType=INTEGER}
  </select>



</mapper>